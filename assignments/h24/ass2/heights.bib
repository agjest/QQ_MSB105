@article{bobbitt-zeher2007,
  title = {The {{Gender Income Gap}} and the {{Role}} of {{Education}}},
  author = {{Bobbitt-Zeher}, Donna},
  year = {2007},
  month = jan,
  journal = {Sociology of Education},
  volume = {80},
  number = {1},
  pages = {1--22},
  publisher = {{SAGE Publications Inc}},
  issn = {0038-0407},
  urldate = {2023-10-06},
  abstract = {Education is thought to be the pathway to success for disadvantaged groups. Given that young women now match or surpass men's educational achievements on many measures, how do they fare in terms of equal earnings? Would further educational changes matter for closing any existing gap? Analyzing data from the National Educational Longitudinal Survey, the author found that college-educated men in their mid-20s already earn, on average, about \$7,000 more per year than do college-educated women. The findings suggest that this gap would still be substantial\textendash about \$4,400 per year\textendash if women and men had similar educational credentials, scores on standardized tests, fields of study, and degrees from colleges of similar selectivity. Although women's gains in education may have been central to narrowing the gender gap in income historically, gender differences in fields of study continue to disadvantage women. Moreover, gender differences in work-related factors are more important than are educational differences for understanding contemporary income inequality among young workers.},
  langid = {english}
}

@article{bockerman2019,
  title = {The Effect of Weight on Labor Market Outcomes: {{An}} Application of Genetic Instrumental Variables},
  shorttitle = {The Effect of Weight on Labor Market Outcomes},
  author = {B{\"o}ckerman, Petri and Cawley, John and Viinikainen, Jutta and Lehtim{\"a}ki, Terho and Rovio, Suvi and Sepp{\"a}l{\"a}, Ilkka and Pehkonen, Jaakko and Raitakari, Olli},
  year = {2019},
  month = jan,
  journal = {Health Economics},
  volume = {28},
  number = {1},
  pages = {65--77},
  issn = {1057-9230},
  urldate = {2023-10-06},
  abstract = {This paper contributes to the literature on the labor market consequences of obesity by using a novel instrument: genetic risk score, which reflects the predisposition to higher body mass index (BMI) across many genetic loci. We estimate instrumental variable models of the effect of BMI on labor market outcomes using Finnish data that have many strengths, for example, BMI that is measured rather than self-reported, and data on earnings and social income transfers that are from administrative tax records and are thus free of the problems associated with nonresponse, reporting error or top coding. The empirical results are sensitive to whether we use a narrower or broader genetic risk score, and to model specification. For example, models using the narrower genetic risk score as an instrument imply that a one-unit increase in BMI is associated with 6.9\% lower wages, 1.8\% fewer years employed, and a 3 percentage point higher probability of receiving any social income transfers. However, when we use a newer, broader genetic risk score, we cannot reject the null hypothesis of no effect. Future research using genetic risk scores should examine the sensitivity of their results to the risk score used.},
  pmcid = {PMC6585973},
  pmid = {30240095}
}

