@article{osland2020,
  title = {House Price Valuation of Environmental Amenities: {{An}} Application of {{GIS-derived}} Data},
  shorttitle = {House Price Valuation of Environmental Amenities},
  author = {Osland, Liv and {\"O}sth, John and Nordvik, Viggo},
